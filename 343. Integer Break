//观察就能知道拆出足够多的 3 就能使得乘积最大   http://blog.csdn.net/liyuanbhu/article/details/51198124
class Solution {
public:
    int integerBreak(int n) {
        if(n==2)   return 1;
        if(n==3)   return 2;
        int res=1;
        while(n>4){
            res*=3;
            n-=3;
        }
        return res*n;
    }
};
